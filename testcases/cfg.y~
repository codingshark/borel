
%type <> rule
%type <> productions_list
%type <> production_n_hash
%type <> production
%type <> symbol


%token <string> name
%token <int> cfg_type
%token <long int> hash_code
%%

rule:
	name productions_list
	;

symbol:
	name
|	cfg_type
	;

productions_list:
	productions_list production_n_hash
|	production_n_hash
	;

production_n_hash:
	production hash_code
	;

production:
	production symbol
|	symbol
	;

