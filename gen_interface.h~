#ifndef GEN_INTERFACE_H_
#define GEN_INTERFACE_H_

#include "node_list.h"

class gen_interface//this is an interface for the back end of compiler
{
	public:
		gen_interface (const node_list & a_cfg);
		virtual ~gen_interface ();

		virtual void code_gen()=0;
	protected:
		const node_list & cfg;//context free grammar
};

#endif
